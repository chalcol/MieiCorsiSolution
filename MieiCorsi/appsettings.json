{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "emmet.includeLanguages": { "aspnetcorerazor": "html" },
  "browserSync.config": {
    "proxy": "localhost:5001",
    "files": [ "View/**/*.cshtml", "wwwroot/**/*" ]
  },
  "fontAwesomeAutocomplete.patterns": [ "**/*.cshtml" ],
  "fontAwesomeAutocomplete.triggerWord": "f"

}
